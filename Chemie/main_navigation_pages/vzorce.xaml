        <ComboBox Name="fncpick" VerticalAlignment="Top" Width="340" Height="25" FontSize="14" Cursor="Hand" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalContentAlignment="Center">
            <ComboBoxItem x:Name="hm" Content="{DynamicResource hm}"></ComboBoxItem>
            <ComboBoxItem x:Name="hol" Content="{DynamicResource hol}"></ComboBoxItem>
            <ComboBoxItem x:Name="oz" Content="{DynamicResource oz}"></ComboBoxItem>
            <ComboBoxItem x:Name="lm" Content="{DynamicResource lm}"></ComboBoxItem>
            <ComboBoxItem x:Name="mk" Content="{DynamicResource mk}"></ComboBoxItem>
            <ComboBoxItem x:Name="rr" Content="{DynamicResource rr}"></ComboBoxItem>
            <ComboBoxItem x:Name="chr" Content="{DynamicResource chr}"></ComboBoxItem>
        </ComboBox>
    </Grid>
</Page>

